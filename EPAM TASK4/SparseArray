import java.io.*;

import java.util.*;


public class Solution {

    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        ArrayList<String> al=new ArrayList<String>();
        int n=sc.nextInt();
        for(int i=0;i<n;i++)
        {
            al.add(sc.next());
        }
        int m=sc.nextInt();
        for(int i=0;i<m;i++)
        {
            System.out.println(Collections.frequency(al,sc.next()));
        }
    }
}
